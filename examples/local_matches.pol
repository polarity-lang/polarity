data Nat {
    Z,
    S(n: Nat)
}

data Bool {
    True,
    False
}

codata Fun(a b: Type) {
    Fun(a, b).ap(a: Type, b: Type, x: a) : b
}

data Top {
    Unit
}

def Top.top_is_zero(n: Nat): Bool {
    Unit => n.match is_zero {
        Z => True,
        S(n) => False,
    }
}

def Top.top_id(a: Type): Fun(a, a) {
    Unit => comatch Id {
        ap(a, b, x) => x
    }
}
