def .add(y) {
    Z => y,
    S(x') => S(x'.add(y))
}

def .head(n) {
    Cons(n', x, xs) => x,
    Nil absurd
}

def .tail(n) {
    Cons(n', x, xs) => xs,
    Nil absurd
}

def .append(n, m, ys) {
    Nil => ys,
    Cons(n', x, xs) => Cons(n'.add(m), x, xs.append(n', m, ys))
}

def .example1 { Unit => Cons(S(Z), Z, Cons(Z, Z, Nil)) }

let main {Unit.example1.append(S(S(Z)), S(S(Z)), Unit.example1)}
