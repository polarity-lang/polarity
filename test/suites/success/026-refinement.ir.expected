def x0.sym(x, y) { Refl(x0) => Refl(y) }

def x0.subst(x, y, p) { Refl(x0) => p }

def x0.trans(x, y, z, h) { Refl(x0) => h }

def x0.cong(x, y, f) { Refl(x0) => Refl(f.ap(y)) }

def x.not {
    True => False,
    False => True
}

def self.not_inverse {
    True => Refl(True),
    False => Refl(False)
}
