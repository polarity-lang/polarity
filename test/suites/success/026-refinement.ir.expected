def .sym(a, x, y) { Refl(a0, x0) => Refl(a, y) }

def .subst(a, x, y, p) { Refl(a0, x0) => p }

def .trans(a, x, y, z, h) { Refl(a0, x0) => h }

def .cong(a, b, x, y, f) { Refl(a0, x0) => Refl(b, f.ap(a, b, y)) }

def .not {
    True => False,
    False => True
}

def self.not_inverse {
    True => Refl(<ZST>, True),
    False => Refl(<ZST>, False)
}
