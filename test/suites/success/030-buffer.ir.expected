def .head(n) {
    Cons(n', x, xs) => x,
    Nil absurd
}

def .tail(n) {
    Cons(n', x, xs) => xs,
    Nil absurd
}

codef Empty { .read(n) absurd }

codef FromVec(n, xs) {
    .read(n') =>
        comatch {
            .proj1 => xs.head(n'),
            .proj2 => FromVec(n', xs.tail(n'))
        }
}
