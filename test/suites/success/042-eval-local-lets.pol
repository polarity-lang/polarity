data Nat {
    Z,
    S(x: Nat)
}

data Eq(a: Type, x y: a) {
    Refl(a: Type, x: a): Eq(a, x, x),
}

def Nat.add(y: Nat): Nat {
    Z => y,
    S(x') => S(x'.add(y))
}

#[transparent]
let two_plus_three: Nat {
    let x: Nat := S(S(Z));
    let y: Nat := S(S(S(Z)));
    x.add(y)
}

let two_plus_three_equals_five: Eq(Nat, two_plus_three, S(S(S(S(S(Z)))))) {
    Refl(Nat, S(S(S(S(S(Z))))))
}
