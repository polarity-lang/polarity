data Eq (a: Type, x y: a) {
    Refl(a: Type, x: a) : Eq(a, x, x)
}

codata Bool {
    (x: Bool).neg_inverse: Eq(Bool, x, x.not.not),
    and(other: Bool): Bool,
    not: Bool
}
data Unit { Top }

def Unit.example : Bool {
    Top => comatch {
              neg_inverse => ?,
              and(other) => ?,
              not => ?
           }
}

